# Docker Compose Environment Variables

# Kafka
KAFKA_BROKER_ID=1
KAFKA_ZOOKEEPER_CONNECT=zookeeper:2181

# MongoDB
MONGO_URL=mongodb://mongodb:27017
MONGO_USERNAME=admin
MONGO_PASSWORD=password

# Redis
REDIS_URI=redis://redis:6379

# Process Service
PROCESS_PORT=3000

# Agent Services
AGENT_1_PORT=3001
AGENT_2_PORT=3002

# Kafka Topic
KAFKA_TOPIC=agent-events
